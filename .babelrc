{
  "presets": [
    "react",
    "es2015",
    "stage-0",
    ["env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    "add-module-exports",
    "transform-class-properties",
    ["transform-runtime", {
      "regenerator": true
    }]
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    }
  }
}