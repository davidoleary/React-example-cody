  "name": "studio-cody",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "aws-sdk": "^2.156.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.17.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "config": "^1.28.1",
    "cors": "^2.8.4",
    "css-loader": "0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-async-wrap": "^1.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-agent": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.1.0",
    "mf-auth": "^1.0.1",
    "morgan": "^1.8.2",
    "nodeman": "^1.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.76.0",
    "smb2": "^0.2.7",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "uuid": "^3.1.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "scripts": {
    "start": "nodemon build/server/main.js",
    "start-dev": "NODE_ENV=development node scripts/start.js",
    "start-devserver": "NODE_ENV=development nodemon server --exec babel-node",
    "build": "node scripts/build.js",
    "build-server": "webpack --config ./config/webpack.config.server.prod.js",
    "test": "npm run test-client && npm run test-server",
    "test-client": "NODE_ENV=development mocha --recursive --require babel-core/register --require ./testSetup.js ./src/**/*.spec.js*",
    "test-server": "NODE_ENV=development mocha --watch --recursive --require babel-core/register ./server/**/*.spec.js",
    "test-windows": "SET NODE_ENV=test&&mocha --require babel-core/register --require ./testSetup.js ./src/**/*.spec.js*",
    "coverage": "NODE_ENV=test nyc mocha --require babel-core/register src/**/*.spec.js*",
    "coverage-server": "NODE_ENV=test nyc mocha --require babel-core/register server/test/**/*.spec.js",
    "test-server-windows": "SET NODE_ENV=test&&mocha --require babel-core/register ./server/**/**/*.spec.js",
    "coverage-windows": "SET NODE_ENV=test&&nyc mocha --require babel-core/register src/**/*.spec.js*",
    "coverage-server-windows": "SET NODE_ENV=test&&nyc mocha --require babel-core/register server/**/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-mf": "^1.1.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "nyc": "^11.1.0",
    "proxyquire": "^1.8.0",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "lcov"
    ],
    "require": [
      "babel-core/register"
    ],
    "cache": true,
    "sourceMap": false,
    "instrument": false,
    "all": true
  }
}
